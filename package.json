{
  "name": "Tiger",
  "version": "1.0.0",
  "description": "A sotfware that will post automatic on my facebook's page",
  "main": "main.js",
  "scripts": {
    "start": "electron . --icon='./assets/icons/icon.png'",
    "win32": "electron-packager . --platform=win32 --arch=x64 --icon='./assets/icons/icon.ico' --out=../ElectronBuilds --overwrite",
    "mac": "electron-packager . --platform=darwin --arch=x64 --icon='./assets/icons/icon.icns' --out=../ElectronBuilds --overwrite"
  },
  "keywords": [
    "Electron",
    "Tiger"
  ],
  "author": "Jr Vansuita",
  "license": "Apache-2.0",
  "repository": {
    "type": "git",
    "url": "git@bitbucket.org:boutiqueinfantil/tiger.git"
  },
  "dependencies": {
    "emojionearea": "^3.4.1",
    "fb": "^2.0.0",
    "instagram-private-api": "^0.6.8",
    "jquery": "^3.3.1",
    "mongoose": "^5.2.2",
    "nedb": "^1.8.0",
    "npm": "^6.1.0",
    "semantic-ui": "^2.3.3",
    "semantic-ui-calendar": "0.0.8",
    "short-url": "0.0.1",
    "sortablejs": "^1.7.0",
    "tinycolor2": "^1.4.1"
  },
  "devDependencies": {
    "electron": "^1.7.15",
    "electron-packager": "^10.1.2"
  }
}
